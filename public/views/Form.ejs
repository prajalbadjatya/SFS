<%- include ("partials/header") %>
<%- include ("partials/navbar") %>

<div class="mainContainer2">

<div class="loginContainer2">
    <form class="loginForm" action="/fill/<%=id%>" method="post">
        <span class="loginHeading">Feedback Form</span>
        <h5>Form Id : <%=id%></h5>
        
        

        <div class="form-group emailForm">
           <label for="exampleInputEmail1" class="label">Question 1</label>
           <textarea class="form-control" id="exampleInputEmail1" name="question1" aria-describedby="emailHelp" placeholder="Enter response for Question 1" required></textarea>
        </div>

        <div class="form-group">
           <label for="exampleInputPassword1" class="label">Question 2</label>
           <textarea type="text" class="form-control" id="exampleInputPassword1" name="question2" placeholder="Enter response for Question 2" required></textarea>
        </div>

        <div class="form-group">
            <label for="exampleInputPassword1" class="label">Question 3</label>
            <textarea type="text" class="form-control" id="exampleInputPassword1" name="question3" placeholder="Enter response for Question 3" required></textarea>
        </div>

        <div class="form-group">
            <label for="exampleInputPassword1" class="label">Question 4</label>
            <textarea type="text" class="form-control" id="exampleInputPassword1" name="question4" placeholder="Enter response for Question 4" required></textarea>
        </div>

        <div class="form-group">
            <label for="exampleInputPassword1" class="label">Question 5</label>
            <textarea type="text" class="form-control" id="exampleInputPassword1" name="question5" placeholder="Enter response for Question 5" required></textarea>
        </div>

        <button type="submit" id="SubmitBtn" class="btn btn-success">Submit</button>
    </form>
    
</div>

</div>